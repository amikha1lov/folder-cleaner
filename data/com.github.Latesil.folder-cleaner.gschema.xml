<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="com.github.Latesil.folder-cleaner" path="/com/github/Latesil/folder-cleaner/">
    <key type="i" name="count">
      <default>0</default>
      <summary>Count active folders</summary>
      <description>How many folders in app.</description>
    </key>
    <key type="b" name="is-sorted">
      <default>false</default>
      <summary>Is files sorted</summary>
      <description>Only for internal usage.</description>
    </key>
    <key type="b" name="sort-by-category">
      <default>false</default>
      <summary>File sort by category</summary>
      <description>Determines if files should be sorted by category instead by extension.</description>
    </key>
    <key name="saved-folders" type="as">
			<default>[]</default>
			<summary>Saved folders list</summary>
			<description>A list of the folders which are saved.</description>
		</key>
  </schema>
</schemalist>